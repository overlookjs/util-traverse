{
  "name": "@overlook/util-traverse",
  "version": "0.1.0",
  "description": "Overlook framework utility to traverse router tree",
  "main": "index.js",
  "module": "es/index.js",
  "exports": {
    ".": {
      "import": "./es/index.js",
      "require": "./index.js"
    },
    "./es": "./es/index.js"
  },
  "files": [
    "lib/**/*.js",
    "es/**/*.js"
  ],
  "sideEffects": false,
  "author": {
    "name": "Overlook Motel"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/overlookjs/util-traverse.git"
  },
  "bugs": {
    "url": "https://github.com/overlookjs/util-traverse/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@overlook/route": "^0.6.4",
    "@overlookmotel/eslint-config": "^7.2.1",
    "@overlookmotel/eslint-config-jest": "^4.1.1",
    "@overlookmotel/eslint-config-node": "^2.1.2",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^26.4.2",
    "jest-extended": "^0.11.5",
    "parse-node-version": "^1.0.1"
  },
  "keywords": [
    "overlook",
    "traverse",
    "tree"
  ],
  "scripts": {
    "test": "npm run lint && npm run test-main",
    "lint": "eslint . --ext 'js,mjs'",
    "lint-fix": "eslint . --ext 'js,mjs' --fix",
    "test-main": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test-prod": "cross-env NODE_ENV=production npm run test-main",
    "cover": "npm run cover-main && rm -rf coverage",
    "coveralls": "npm run cover-main && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "cover-main": "cross-env COVERAGE=true node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "ci": "if [ $COVERAGE ]; then npm run coveralls; else npm test; fi"
  },
  "engines": {
    "node": ">=10"
  },
  "readmeFilename": "README.md",
  "license": "MIT"
}
